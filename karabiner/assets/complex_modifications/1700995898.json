{
    "title": "KonKen's Setup: Quick App Launchers for Mac",
    "rules": [
        {
            "description": "s & p (simultaneously for 0.25 sec) => Open System Preferences",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "simultaneous": [
                            {
                                "key_code": "s"
                            },
                            {
                                "key_code": "p"
                            }
                        ]
                    },
                    "to_if_held_down": [
                        {
                            "shell_command": "open -a 'System Preferences'"
                        }
                    ],
                    "parameters": {
                        "basic.simultaneous_threshold_milliseconds": 25,
                        "basic.to_if_held_down_threshold_milliseconds": 250
                    }
                }
            ]
        },
        {
            "description": "a & s (simultaneously for 0.25 sec) => Open App Store",
